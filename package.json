{
  "name": "@flexbe/sdk",
  "version": "0.2.36",
  "description": "TypeScript SDK for Flexbe API",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "browser": "dist/browser/index.js",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "scripts": {
    "build": "npm run build:esm && npm run build:cjs && npm run build:browser",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:browser": "tsc -p tsconfig.browser.json",
    "dev": "tsc -p tsconfig.esm.json --watch",
    "test": "dotenv -e test/.env.test jest",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint"
  },
  "keywords": [
    "flexbe",
    "sdk",
    "typescript",
    "api-client"
  ],
  "author": "Flexbe",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flexbe/sdk-ts.git"
  },
  "bugs": {
    "url": "https://github.com/flexbe/sdk-ts/issues"
  },
  "homepage": "https://github.com/flexbe/sdk-ts#readme",
  "devDependencies": {
    "@flexbe/eslint-config": "^1.0.7",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.19",
    "dotenv-cli": "^7.4.1",
    "eslint": "^9.26.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "typescript": "~5.5.0"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
