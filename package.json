{
    "name": "@flexbe/sdk-ts",
    "version": "0.1.1",
    "description": "TypeScript SDK for Flexbe API",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "build": "tsc",
        "test": "dotenv -e test/.env.test jest",
        "lint": "eslint src --ext .ts",
        "format": "prettier --write \"src/**/*.ts\"",
        "prepare": "npm run build",
        "prepublishOnly": "npm test && npm run lint",
        "publish": "npm publish --access public"
    },
    "keywords": [
        "flexbe",
        "sdk",
        "typescript",
        "api-client"
    ],
    "author": "Flexbe",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/flexbe/sdk-ts.git"
    },
    "bugs": {
        "url": "https://github.com/flexbe/sdk-ts/issues"
    },
    "homepage": "https://github.com/flexbe/sdk-ts#readme",
    "dependencies": {
        "axios": "^1.6.7"
    },
    "devDependencies": {
        "@types/jest": "^29.5.12",
        "@types/node": "^20.11.19",
        "@typescript-eslint/eslint-plugin": "^7.0.1",
        "@typescript-eslint/parser": "^7.0.1",
        "dotenv-cli": "^7.4.1",
        "eslint": "^8.56.0",
        "jest": "^29.7.0",
        "prettier": "^3.2.5",
        "ts-jest": "^29.1.2",
        "typescript": "~5.5.0"
    },
    "files": [
        "dist",
        "README.md"
    ],
    "engines": {
        "node": ">=16.0.0"
    },
    "publishConfig": {
        "access": "public"
    }
}