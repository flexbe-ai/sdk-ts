{
    "name": "@flexbe/sdk-ts",
    "version": "0.1.0",
    "description": "TypeScript SDK for Flexbe API",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "build": "tsc",
        "test": "jest",
        "lint": "eslint src --ext .ts",
